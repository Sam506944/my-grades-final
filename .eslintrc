{
  // "plugins": [],
  "env": {
    "browser": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2020,
    "ecmaFeatures": {
      // "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "for-direction": 2,
    "getter-return": 2,
    "no-async-promise-executor": 1,
    "no-await-in-loop": 1,
    "no-compare-neg-zero": 2,
    "no-cond-assign": 1,
    "no-constant-condition": 1,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-else-if": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-import-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 1,
    "no-misleading-character-class": 1,
    "no-obj-calls": 1,
    "no-prototype-builtins": 1,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 2,
    "no-template-curly-in-string": 1,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 1,
    "require-atomic-updates": 1,
    "use-isnan": 2,
    "valid-typeof": 1,
    "array-callback-return": 2,
    "consistent-return": 1,
    "dot-location": 1,
    "dot-notation": 1,
    "eqeqeq": 1,
    "grouped-accessor-pairs": 1,
    "no-alert": 1,
    "no-case-declarations": 2,
    "no-constructor-return": 1,
    "no-else-return": 1,
    "no-empty-pattern": 1,
    "no-eq-null": 1,
    "no-eval": 1,
    "no-extend-native": 1,
    "no-extra-label": 1,
    "no-fallthrough": 2,
    "no-global-assign": 1,
    "no-implied-eval": 1,
    "no-invalid-this": 2,
    "no-lone-blocks": 1,
    "no-multi-spaces": 2,
    "no-new": 1,
    "no-new-func": 2,
    "no-new-wrappers": 1,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 0,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-return-await": 1,
    "no-script-url": 2,
    "no-self-assign": 1,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-throw-literal": 2,
    "no-unused-expressions": 1,
    "no-unused-labels": 1,
    "no-useless-catch": 1,
    "no-useless-concat": 1,
    "no-useless-escape": 1,
    "no-useless-return": 1,
    "no-void": 1,
    "prefer-named-capture-group": 1,
    "require-await": 1,
    "vars-on-top": 2,
    "wrap-iife": 1,
    "yoda": 1,
    "no-delete-var": 1,
    "no-label-var": 2,
    "no-shadow": 1,
    "no-shadow-restricted-names": 2,
    "no-undef": 2,
    "no-undef-init": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 2,
    "array-bracket-newline": [1, "consistent"],
    "array-element-newline": [1, "consistent"],
    "block-spacing": 2,
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "camelcase": 1,
    "capitalized-comments": [0, "never"],  // off right now
    "comma-spacing": 2,
    "comma-style": [1, "last"],
    "computed-property-spacing": 1,
    "func-call-spacing": 1,
    "func-names": [ 2, "never"],
    "function-paren-newline": [1, "consistent"],
    "implicit-arrow-linebreak": 1,
    "indent": [2, 2, { "SwitchCase": 1, "VariableDeclarator": "first" }],
    "jsx-quotes": 2,
    "key-spacing": 2,
    "keyword-spacing": 1,
    "new-cap": 2,
    "new-parens": 2,
    "no-array-constructor": 2,
    "no-bitwise": 1,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": 2,
    "no-negated-condition": 1,
    "no-nested-ternary": 1,
    "no-new-object": 1,
    "no-tabs": 2,
    "no-trailing-spaces": 1,
    "no-unneeded-ternary": 1,
    "no-whitespace-before-property": 2,
    "nonblock-statement-body-position": 2,
    "object-curly-spacing": [1, "always", { "objectsInObjects": true }],
    "operator-assignment": 1,
    "operator-linebreak": [1, "before"],
    "prefer-exponentiation-operator": 2,
    "prefer-object-spread": 1,
    "quote-props": [1, "as-needed"],
    "quotes": [1, "single"],
    "semi": [2, "always"],
    "semi-spacing": 2,
    "semi-style": 2,
    "space-before-blocks": 2,
    "space-before-function-paren": [2,
      {
        "anonymous": "always",
        "named": "never",
        "asyncArrow": "always"
      }
    ],
    "space-in-parens": 1,
    "space-infix-ops": 2,
    "space-unary-ops": [2, { "words": true, "nonwords": false }],
    "spaced-comment": [2, "always", { "exceptions": ["-"] }],
    "switch-colon-spacing": 2,
    "template-tag-spacing": 2,
    "wrap-regex": 2,
    "arrow-body-style": 2,
    "arrow-spacing": 2,
    "constructor-super": 2,
    "no-class-assign": 2,
    "no-confusing-arrow": 1,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-imports": 1,
    "no-new-symbol": 2,
    "no-this-before-super": 2,
    "no-useless-computed-key": 1,
    "no-var": 1,
    "object-shorthand": [2, "always"],
    "prefer-arrow-callback": 2,
    "prefer-const": 1,
    "prefer-destructuring": 0,
    "prefer-rest-params": 1,
    "prefer-spread": 2,
    "prefer-template": 1,
    "require-yield": 1,
    "rest-spread-spacing": 2,
    "symbol-description": 1,
    "template-curly-spacing": 2,
    "yield-star-spacing": 2,
    "one-var": [1, "never"]
  }
}
